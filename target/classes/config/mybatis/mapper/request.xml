<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="r">

	<!-- 희망도서 추가하기 -->
	<insert id="request_insert" parameterType="request">
		insert into request
		values(
		#{id},
		#{title},
		#{authors},
		#{isbn},
		#{opinion}
		)
	</insert>
	
</mapper>
